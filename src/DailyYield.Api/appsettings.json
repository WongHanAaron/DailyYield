{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=dailyyield;Username=postgres;Password=password"
  },
  "Jwt": {
    "Key": "your-secret-key-here",
    "Issuer": "DailyYield.Api",
    "Audience": "DailyYield.Client"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
